<Window x:Class="AiGumsPC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="700" Width="700">
    <Grid Margin="0,0,0,0">
        <StatusBar HorizontalAlignment="Left" Height="36" Margin="0,634,-0.4,0" VerticalAlignment="Top" Width="694"/>
        <TabControl HorizontalAlignment="Left" Height="634" VerticalAlignment="Top" Width="694" Margin="0,0,-0.4,0">
            <TabItem Header="Alta MPAT">
                <Grid Background="#00FF00" Margin="0,0,0,0.4">
                    <Label x:Name="lbTestFood" Content="Test Food" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="lbProcedimiento" Content="Procedimiento" HorizontalAlignment="Left" Margin="10,89,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.586,1.57"/>
                    <ComboBox x:Name="cbTextFood" HorizontalAlignment="Left" Margin="128,61,0,0" VerticalAlignment="Top" Width="322"/>
                    <TextBox HorizontalAlignment="Left" Height="78" Margin="128,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="322"/>
                    <Button x:Name="btAltaTestFood" Content="Alta" HorizontalAlignment="Left" Margin="471,61,0,0" VerticalAlignment="Top" Width="75" Click="btAltaTestFood_Click"/>
                    <Button x:Name="btBajaTestFood" Content="Baja" HorizontalAlignment="Left" Margin="560,61,0,0" VerticalAlignment="Top" Width="75"/>
                    <Label x:Name="lbCiclosMasticatorios" Content="Ciclos Masticatorios" HorizontalAlignment="Left" Margin="10,171,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="tbCiclosMasticatorios" HorizontalAlignment="Left" Height="23" Margin="130,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="320"/>
                    <Label x:Name="lbCiclos" Content="Ciclos de Evaluación" HorizontalAlignment="Left" Margin="10,199,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="tbCiclosMasticatorios_Copy" HorizontalAlignment="Left" Height="23" Margin="130,199,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="320"/>

                </Grid>

            </TabItem>
            <TabItem Header="Alta Experimento">
                <Grid Background="#000F00">
                </Grid>
            </TabItem>
            <TabItem Header="Alta Diagnóstico">
                <Grid Background="#000F00">
                </Grid>
            </TabItem>
            <TabItem Header="Listado MPATs">
                <Grid Background="#000F00">
                </Grid>
            </TabItem>
            <TabItem Header="Listado Experimentos">
                <Grid Background="#000F00">
                </Grid>
            </TabItem>
            <TabItem Header="Listado Diagnósticos">
                <Grid Background="#000F00">
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
