<Window x:Class="AiGumsPC.AltaTipoTestFood"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AltaTipoTestFood" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="tbNombre" HorizontalAlignment="Left" Height="23" Margin="104,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170"/>
        <TextBox x:Name="tbDescripcion" HorizontalAlignment="Left" Height="23" Margin="104,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="10,12,0,0" VerticalAlignment="Top" Width="89"/>
        <Label Content="Descripción" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="89"/>
        <Button Content="Grabar" HorizontalAlignment="Left" Margin="50,166,0,0" VerticalAlignment="Top" Width="87" Click="grabarTipoTestFood" Height="37"/>
        <Button Content="Cancelar" HorizontalAlignment="Left" Margin="158,166,0,0" VerticalAlignment="Top" Width="87" Click="cancelarTipoTestFood" Height="37"/>

    </Grid>
</Window>
